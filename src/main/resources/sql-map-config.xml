<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias alias="member" type="com.springbook.biz.member.MemberVO" />
		<typeAlias alias="goods" type="com.springbook.biz.goods.GoodsVO" />
		<typeAlias alias="notice" type="com.springbook.biz.notice.NoticeVO" />
	</typeAliases>
	
	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/member-mapping.xml" />
		<mapper resource="mappings/goods-mapping.xml" />
		<mapper resource="mappings/notice-mapping.xml" />
	</mappers>
</configuration>
